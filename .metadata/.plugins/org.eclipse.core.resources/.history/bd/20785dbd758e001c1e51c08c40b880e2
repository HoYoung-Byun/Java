package data_and_operations;

public class StringApp {

	public static void main(String[] args) {
		System.out.println("Hello World"); // String 문자열(Character의 집합)
		System.out.println('Hello World'); // Character 문자(1글자)
		
		System.out.println("Hello \nworld"); // new line (\n)
		System.out.println(" \" Hello world \" "); // escape 특수문자 처리 (\)

	}

}
